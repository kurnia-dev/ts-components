{
  "name": "ts-vue-components",
  "version": "0.2.1",
  "type": "module",
  "description": "Tag Samurai's Vue Components",
  "exports": "./dist/ts-vue-components.es.js",
  "module": "dist/ts-vue-components.es.js",
  "unpkg": "dist/ts-vue-components.umd.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "src/**/*.d.ts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test": "vitest --coverage",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "build:pub": "npm run build && npm link && npm publish",
    "generate:types": "vue-tsc --declaration --emitDeclarationOnly --outdir ./dist",
    "cypress:component": "cypress open --component --browser chrome"
  },
  "keywords": [],
  "author": "Kurnia-dev",
  "license": "ISC",
  "dependencies": {
    "@vueuse/head": "^2.0.0",
    "primeicons": "^6.0.1",
    "primevue": "3.29.2",
    "remixicon": "^4.1.0",
    "vee-validate": "^4.12.4",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@cypress/vue": "^6.0.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jest": "^29.5.11",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitest/coverage-v8": "^1.2.1",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.0",
    "cypress": "^13.6.1",
    "eslint": "^8.49.0",
    "eslint-config-ts-code-standard": "^1.0.2",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.17.0",
    "highlight.js": "^11.9.0",
    "jsdom": "^23.0.1",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "rfs": "^10.0.0",
    "sass": "^1.70.0",
    "start-server-and-test": "^2.0.3",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.7.1",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.25"
  }
}
